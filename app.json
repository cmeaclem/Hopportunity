{
  "expo": {
    "name": "Hopportunity",
    "slug": "Hopportunity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/kangaroo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/kangaroo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.meaclem.hopportunity",
      "adaptiveIcon": {
        "foregroundImage": "./assets/kangaroo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.HIGH_SAMPLING_RATE_SENSORS"
      ]
    },
    "web": {
      "favicon": "./assets/kangaroo.png"
    }
  }
}
